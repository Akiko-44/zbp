	<script>
		var $slider = null;

		var mainVm = new Vue({
			el: "#main",
			data: {
				homeModule: [],
				advertiseList: [],
				moduleGroupGoods: {},
				modulePromoteGoods: {},
				moduleNewGoods: {},
				moduleContent: {},
				moduleMerchant: {},
				dmBannerDTOList: [],
				categoryShow: [],
				brand: [],
				admin: [],
				flagShip: [],
				hotSellGoods: [],
				patentGoods: [],
				newGoods: [],
				burstGoods: [],
				cateBrand: [],
				cateBrandRow: [],
				num: 0,
				notice: [],
				SearchSeven: [],
				systemDate: '',
				background: {},
				timeOut: []
			},
			created() {

			},

			mounted: function () {
				this.getHomePage()
				this.getBackground()
				var self = this
				this.showMarquee(this.num);
				this.CurrentTime()
				setTimeout(function () {
					self.getCategoryShow()
				}, 500)

			},
			methods: {
				getHomePage() {
					if (localStorage.getItem('homePageStorage')) {
						this.setHomePage(JSON.parse(localStorage.getItem('homePageStorage')))
					}
					utils.ajax({
						type: 'get',
						url: '/app/homePage/new',
						success: (data) => {
							this.setHomePage(data)
							localStorage.setItem('homePageStorage', JSON.stringify(data))
						}
					})
				},
				setHomePage(data) {
					this.notice = data.data.platformNotice.records
					this.dmBannerDTOList = data.data.banner
					this.SearchSeven = data.data.todayGuidePrice
					this.homeModule = data.data.homeModule
					this.homeModule.map(item => {
						if (item.id === '1000') {
							this.moduleGroupGoods = item
						} else if (item.id === '1001') {
							this.moduleNewGoods = item
						} else if (item.id === '1002') {
							this.moduleContent = item
						} else if (item.id === '1003') {
							this.moduleMerchant = item
						} else if (item.id === '1017') {
							this.modulePromoteGoods = item
							this.modulePromoteGoods.goodsVOList.map((goodsItem, goodsIndex) => {
								goodsItem.countDown = goodsItem.endTime
								this.getCountDown(goodsItem.endTime, goodsIndex)
							})
						}
					})
					this.advertiseList = data.data.advertise
				},
				// 背景图配置
				getBackground() {
					utils.ajax({
						type: 'get',
						url: '/api/backgroundImg/getbackgroundImg',
						data: { backgroundImgType: 1 },
						success: (res) => {
							this.background = res.data
							if (this.background.wholeType === 1 && this.background.wholeColor) {
