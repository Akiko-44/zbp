      <button class="btn btn-danger" onclick="$('.ensureOrderDialog').removeClass('show')">取消</button>
      <button class="btn btn-primary ensureBtn" id="ensureBtn">确定</button>
    </div>
  </div>
</div>
<script src="/src/js/imgFileupload.js"></script>
<script>
  $(".userLeftNav li[data-target='intellectual']").addClass("cur");
  $(".et_form_item").hide()
  $(".addimg").hide()
  var app = new Vue({
    el: '#Intellectual',
    data: {
      list: [],
      titleVal: '',
      addContent: false,
      textarea: '',
      Img: [],
    },
    mounted() {
      this._GetIntellectualItem()
    },
    methods: {
      _GetIntellectualItem() {
        var that = this;
        utils.ajax({
          url: '/api/intellectual/page',
          type: "get",
          data: {
            limit: 5,
            offset: 1
          },
          success: function (res) {
            that.list = res.data.records;
          }
        })
      },
      del(id) {
        $(".ensureOrderDialog").addClass("show");
        var that = this;
        $(document).on("click", ".ensureBtn", function (e) {
          // 确认删除知识产权
          utils.ajax({
            url: '/api/intellectual/delete/' + id,
            type: "post",
            success: function (res) {
              that.list.slice(id, 1);
              utils.alert("success", "删除成功");
              $(".ensureOrderDialog").removeClass("show");
              that._GetIntellectualItem()
            }
          })
        })

      },
      close(index) {
        console.log('index', index);
        this.Img.splice(index, 1)
      },
      addE() {
        this.addContent = true;
        this.Img = [];
        $(".et_form_item").show();
        this.textarea = this.titleVal = "";
        $(".imgAll ul").html("")
      },
      submit(e) {
        // if (!this.titleVal || !this.textarea) return
        if (!this.titleVal) {
          // alert("请输入标题")
          utils.alert("danger", "请输入标题")
          return
        }
        if (!this.textarea) {
          // alert("请输入内容")
          utils.alert("danger", "请输入内容")
          return
        }
        var number = $(".imgAll ul").children().length;
        var arr = [];
        if (number == 0) {
          // alert("请上传图片")
          utils.alert("danger", "请上传图片")
          return
        }
        var that = this
        // console.log(number);
        if (number != 0) {
          var imglist = $(".upload");
          for (var index = 0; index < imglist.length; index++) {
            arr.push($(imglist[index]).attr("src"))
          }
          var arry = arr.join(',');
        }
        var data = {
          name: this.titleVal,
          detail: this.textarea,
          pics: this.Img.join(",")
        }
        utils.ajax({
          url: "/api/intellectual/saveOrUpdate",
          type: "post",
          data: JSON.stringify(data),
          el: e.target,
          success: function (res) {
            // console.log('rsdses', res);
            if (res.code == 200) {
              // console.log('res.code', res);
              // alert("成功")
              utils.alert("success", "操作成功");
              that.addContent = false;
              that._GetIntellectualItem()
              $(".et_form_item").hide()
            }
          }
        })
      },
      uploadGoodsImg(e) {
        let inputfile = this.$refs.inputfile
        console.log('cc', inputfile.files[0]);
        var formFile = this.formatUpLoadImg(inputfile.files[0], 2)
        if (formFile && this.Img && this.Img.length < 3) {
          this.Img.push(this._TemplateUploadImgAjax("post", "/api/pic/upload", formFile))
        } else {
          console.log('上传');
        }
      },
      // 格式化图片
      formatUpLoadImg(_file, _fileSize) {
        var formFile = new FormData();
        var file = _file;
        var fileSize = (file.size / 1024 / 1024).toFixed(0);
        if (fileSize > _fileSize) {
          alert("上传的文件不能超过" + _fileSize + "M")
          return
        }
        formFile.append("file", file); //加入文件对象
        return formFile
      },
      // data类型FormData
      _TemplateUploadImgAjax(type, url, data) {
        var that = this;
        var params;
        $.ajax({
          url: url,
          data: data,
          type: type,
          async: false,
          cache: false,
          contentType: false, //不设置内容类型
          processData: false, //不处理数据
          success: function (res) {
            params = res.data.picUrl
          }
        })
        return params
      },
    },
  });
</script>

</html>