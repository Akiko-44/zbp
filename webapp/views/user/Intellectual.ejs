            </el-breadcrumb-item>
            <el-breadcrumb-item :to="{ path: '/' }">
              <a href="/user/acc_info">个人中心</a>
            </el-breadcrumb-item>
            <el-breadcrumb-item>知识产权保护</el-breadcrumb-item>
          </el-breadcrumb>
        </div>
      </div>
      <div class="container clearfix">
        <div class="userLeftNav">
          <%- include userLeftNav.ejs %>
        </div>
        <div class="userRightContent" v-if="addContent">
          <!-- <p class="add">添加知识产权</p> -->
          <div class="addtitle clearfix">
            <p class="add fl">添加知识产权</p>
          </div>
          <div class="contain">
            <div class="title clearfix">
              <p class="need fl">标题：</p>
              <input class="accountTitle  fl" type="text" v-model="titleVal" placeholder="请输入标题" maxlength="30">
            </div>
            <div class="desc clearfix">
              <p class="need fl">详情：</p>
              <textarea class="descinfo fl" v-model="textarea" value="" placeholder="请填写说明..."
                placeholder-style="color:#999" placeholder-class="textarea-placeholder" maxlength="140"
                auto-focus="false" focus="false" auto-height="" bindinput="" bindfocus="" bindblur="" bindconfirm=""
                bindlinechange="%{15}">
                                    </textarea>
            </div>
            <div class="desc clearfix">
              <p class="need fl">上传图片：</p>
              <div class="box">
                <div class="imgFileUploade">
                  <img class="add_icon" src="/src/img/hx_img/add.png" alt="">
                  <input type="file" id="uploadimg" name="cloudfile" ref="inputfile" accept="image/*"
                    @change="uploadGoodsImg(this)">
                  <div class="imgAll">
                    <ul>
                      <li v-for="(item,index) in Img">
                        <img class="close" @click="close(index)" src="/src/img/hx_img/closeImg.png" alt="">
                        <img :src="item" alt="">
                      </li>
                    </ul>
                  </div>
                </div>
              </div>
            </div>
            <p class="imgtext"> 最多可上传3张凭证，不可超过2M，支持格式JPG、PNG、JPGE等格式</p>
          </div>
          <div class="submit" @click="submit($event)">保存</div>
        </div>
        <div class="userRightContent" v-else>
          <div class="addtitle clearfix">
            <p class="add fl">知识产权保护</p>
            <div class="submit submittop fr" @click="addE()">添加</div>
          </div>
          <div class="no-content tc" style="padding:100px 0;color:#333;font-size:16px;" v-if="list && !list.length">
            <img src="/src/img/user/no_content.png" width="482" alt="">
            <p style="margin-top:-50px">暂无内容</p>
          </div>
          <template>
            <div class="Intellectual_container" v-for="(item ,index) in list">
              <p class="clearfix"><span class="fl">{{item.name}}</span><span class="delete fr"
                  @click="del(item.id)">删除</span></p>
              <p class="time">{{item.createTime}}</p>
              <div class="showimg">
                <img :src="ittem" alt="" v-for="(ittem,index) in item.pics" v-if="item.pics.length">
                <div v-else></div>
              </div>
              <p class="Intellectual_desc">{{item.detail}}</p>
            </div>
          </template>


        </div>
      </div>
  </form>
