                  <img :src="subItem.imgUrl" width="100%" height="100%">
                </div>
                <div class="card-info">
                  <h4 class="card-title text-hidden">{{subItem.goodsName}}</h4>
                  <p class="price">¥<span v-if="subItem.groupPrice">{{subItem.groupPrice.toFixed(2)}}</span><del>
