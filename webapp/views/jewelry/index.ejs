      <div class="container">
        <el-carousel trigger="click" height="460px" loop :interval="4000">
          <el-carousel-item v-for="(item,i) in bannerList" :key="i">
            <img :src="item.pcPicture" @click="goBannerUrl(item)" style="cursor: pointer;" />
