        </div>
      </div>
      <div v-if="advertiseList[1] && advertiseList[1].name" class="advertisement" @click="goBannerUrl(advertiseList[1])"
        :style="{backgroundImage: 'url(' + advertiseList[1].pcPicture + ')'}">
      </div>
      <div class="new-goods card-block" v-if="goodsList.length">
        <h4 class="title">{{goodsName}}<span class="more cur-pointer" @click="goGoodsList(0)">更多 ></h4>
        <div class="goods-box">
          <el-row :gutter="11">
            <el-col :span="4" v-for="(subItem, subIndex) in goodsList" :key="subIndex"
              @click.native="goGoodsDetail(subItem.id)">
              <div class="goods cur-pointer">
                <div class="goods-img-box">
                  <div class="activity-bg" v-if="subItem.pcBigIcon"
                    :style="{backgroundImage: subItem.pcBigIcon ? 'url(' + subItem.pcBigIcon + ')' : ''}"></div>
                  <img :src="subItem.imgUrl" width="100%" height="100%">
                </div>
                <div class="card-info">
                  <h4 class="card-title text-hidden">{{subItem.goodsName}}</h4>
                  <p class="price theme-color fz-12">¥<span class="fz-14"
                      v-if="subItem.price">{{subItem.price.toFixed(2)}}</span></p>
                  <p class="goods-else">
                    <el-row :gutter="4">
                      <el-col :span="4">
                        <img :src="subItem.merchantLogo" width="18">
                      </el-col>
                      <el-col :span="20" class="merchant-name text-hidden">
                        {{subItem.merchantName}}
                      </el-col>
                    </el-row>
                  </p>
                </div>
              </div>
            </el-col>
          </el-row>
        </div>
      </div>
      <div v-if="advertiseList[2] && advertiseList[2].name" class="advertisement" @click="goBannerUrl(advertiseList[2])"
        :style="{backgroundImage: 'url(' + advertiseList[2].pcPicture + ')'}">
      </div>
      <div class="brand-goods card-block" v-if="brandList.length">
        <h4 class="title">{{brandName}}<span class="more cur-pointer" @click="goShopList">更多 ></h4>
        <div class="goods-box">
          <el-row :gutter="11">
            <el-col :span="12" v-for="(subItem, subIndex) in brandList" :key="subIndex"
              @click.native="toGoodsDetail(subItem.id)">
              <div class="brand-item">
                <div class="shop-box" @click='toShopDetail(subItem.id)'>
                  <div class="shop-img"
                    :style="{backgroundImage: 'url(' + subItem.merchantPic + ')', backgroundSize:'cover'}">
                  </div>
                </div>
                <div class="shop-info tc">
                  <h4 class="text-hidden fz-16">{{subItem.merchantName}}</h4>
                  <p class="shop-recommend text-hidden fz-14">{{subItem.recommendDetail}}</p>
                  <!-- <van-button class="shop-btn" round color="#FB746E">进店逛逛 ></van-button> -->
                  <el-button type="danger" class="shop-btn" @click="goMerchantDetail(subItem.id)">进店逛逛 ></el-button>
                </div>
              </div>
              <div class="shop-goods">
                <div class="goods-item cur-pointer" v-for="(goodsItem, goodsIndex) in subItem.goodsVOList"
                  :key="goodsIndex" @click='goGoodsDetail(goodsItem.id)'>
                  <div class="goods-img-box">
                    <div class="activity-bg" v-if="goodsItem.pcBigIcon"
                      :style="{backgroundImage: goodsItem.pcBigIcon ? 'url(' + goodsItem.pcBigIcon + ')' : ''}"></div>
                    <img :src="goodsItem.imgUrl" width="100%">
                  </div>
                  <div class="goods-info">
                    <p class="title text-hidden fz-14">{{goodsItem.goodsName}}</p>
