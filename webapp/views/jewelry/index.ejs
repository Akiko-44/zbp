          <ul class="clearfix">
            <li v-for="(item,index) in list" @click="goto(item.id)" :class="{'cur': form.categoryId == item.id}">
              {{item.catName}}</li>
          </ul>
        </div>
        <div class="new-goods card-block">
          <div class="goods-box clearfix">
            <div class="tc no-goods" v-if="categoryGoodsList.length == 0">
              <img src="../../src/img/common/no-goods.png" />
              <p>暂无商品</p>
            </div>
            <template v-else>
              <div class="goods cur-pointer" v-for="(subItem, subIndex) in categoryGoodsList" :key="subIndex"
                @click="goGoodsDetail(subItem.goodsId)">
                <div class="goods-img-box">
                  <div class="activity-bg" v-if="subItem.pcBigIcon"
                    :style="{backgroundImage: subItem.pcBigIcon ? 'url(' + subItem.pcBigIcon + ')' : ''}"></div>
                  <img :src="subItem.goodsPic" width="100%" height="100%">
                </div>
                <div class="card-info">
                  <h4 class="card-title text-hidden">{{subItem.goodsName}}</h4>
                  <p class="price theme-color fz-12">¥<span class="fz-14"
                      v-if="subItem.lowPrice">{{subItem.lowPrice.toFixed(2)}}<span
                        v-if="subItem.lowPrice !== subItem.hightPrice">~{{subItem.hightPrice.toFixed(2)}}</span></span>
                  </p>
                  <p class="goods-else">
                    <el-row :gutter="4">
                      <el-col :span="4">
                        <img :src="subItem.merchantPic" width="18">
                      </el-col>
                      <el-col :span="20" class="merchant-name text-hidden">
                        {{subItem.merchantName}}
                      </el-col>
                    </el-row>
                  </p>
                </div>
              </div>
            </template>

          </div>
          <div class="tr container" style="padding: 30px 0;" v-show="listPages > 1">
            <div id="pagination" class="pagination"></div>
          </div>
        </div>
      </div>

    </div>
  </div>
  </div>

  <%- include ../footer.ejs %>
  <script>
    new Vue({
      el: "#tobuy",
      data: {
        newGoods: [],
        list: [],
        curCategory: '1184',
        info: '',
        form: {
          offset: 1,
          limit: 15,
          goodsSort: 0,
          categoryId: undefined,
          entrance: 1
        },
        listPages: 0,
        promoteName: '',
        groupName: '',
        goodsName: '',
        brandName: '',
        bannerList: [],
        advertiseList: [],
        moduleList: [],
        zoneList: [],
        promoteList: [],
        groupList: [],
        goodsList: [],
        brandList: [],
        timeOut: [],
        categoryGoodsList: []
      },
      async mounted() {
        this.list = await this._TemplateAjax("get", '/app/homePage/secondCategory', {});
        this.form.categoryId = this.list[0].id
        this.getJewelryHome()
        this.render(1)
      },
      methods: {
        getJewelryHome() {
          if (localStorage.getItem('jewelryHome')) {
            this.setJewelryHome(JSON.parse(localStorage.getItem('jewelryHome')))
          }
          utils.ajax({
            type: 'get',
            url: '/app/homePage/whereBuy/new',
            success: (res) => {
              this.setJewelryHome(res.data)
              localStorage.setItem('jewelryHome', JSON.stringify(res.data))
            }
          })
        },
        setJewelryHome(data) {
          this.bannerList = data.banner
          this.moduleList = data.homeModule
          this.zoneList = data.zone
          let advertiseListTemp = data.advertise
          advertiseListTemp.map((item, index) => {
            if (item.id) {
              this.advertiseList[item.position - 1] = item
            }
          })
          this.moduleList.map(item => {
            if (item.id === '1004') {
              this.promoteName = item.moduleName
              this.promoteList = item.goodsVOList
              this.promoteList.map((goodsItem, goodsIndex) => {
                goodsItem.countDown = goodsItem.endTime
                this.getCountDown(goodsItem.endTime, goodsIndex)
              })
            } else if (item.id === '1005') {
              this.goodsName = item.moduleName
              this.goodsList = item.goodsVOList
            } else if (item.id === '1006') {
              this.groupName = item.moduleName
              this.groupList = item.goodsVOList
            } else if (item.id === '1007') {
              this.brandName = item.moduleName
              this.brandList = item.merchantVOList
            }
          })
          console.log(this.moduleList)
        },
        render(page) {
          const self = this
          self.form.offset = page || 1
          utils.ajax({
            url: '/app/search/jewelryGoodsNew',
            type: 'get',
            data: self.form,
            success: function (response) {
              let resData = response.data
              self.categoryGoodsList = resData.records
              self.listPages = resData.pages
              $("#total").html(resData.total);
              if (self.form.offset == 1) {
                $("#pagination").pagination({
                  pageCount: resData.pages,
                  jump: true,
                  coping: true,
                  current: resData.current,
                  prevContent: '上一页',
                  nextContent: '下一页',
                  callback: function (api) {
                    self.render(api.getCurrent())
                  }
                })
              }
            }
          });
        },
        getCountDown(time, index) {
          let finishTime = +new Date(Date.parse(time.replace(/-/g, "/")))
          let diferentTime = this.leftTimer(finishTime - new Date())
          this.promoteList[index].countDown = diferentTime
          this.timeOut[index] = setInterval(() => {
            let finishTime = +new Date(Date.parse(time.replace(/-/g, "/")))
            let diferentTime = this.leftTimer(finishTime - new Date())
            this.promoteList[index].countDown = diferentTime
            this.$forceUpdate()
          }, 1000)
        },
        goGoodsList(goodsType, zoneId) {
          if (zoneId) {
            window.location = '/jewelry/goodsList?zoneId=' + zoneId
          } else {
            window.location = '/jewelry/goodsList?goodsType=' + goodsType

          }
        },
        goShopList() {
          window.location = '/jewelry/shopList'
        },
        goGoodsDetail(id) {
          window.location = '/jewelry/details?id=' + id
        },
        goMerchantDetail(id) {
          window.location = '/jewelry/shopSearch?id=' + id
        },
        goBannerUrl(data) {
          if (data.linkType === 1) {
            this.goGoodsDetail(data.linkTypeId)
          } else if (data.linkType === 2) {
            this.goMerchantDetail(data.linkTypeId)
          } else if (data.linkType === 3) {
            window.location = '/jewelryCircle/index'
          } else if (data.linkType === 4) {
            window.location = '/jewelryCircle/index'
          } else if (data.pcUrl) {
            window.location = data.pcUrl
          }
        },
        _TemplateAjax(type, url, data) {
          var that = this;
          var params;
          utils.ajax({
            url: url,
            async: false,
            data: data,
            type: type,
            success: function (res) {
              params = res.data
            }
          })
          return params
        },
        goto(categoryId) {
          this.form.categoryId = categoryId
          this.render(1)
        },
        leftTimer(leftTime) {
          var text = "";
          var days = parseInt(leftTime / 1000 / 60 / 60 / 24, 10); //计算剩余的天数
          var hours = parseInt(leftTime / 1000 / 60 / 60 % 24, 10); //计算剩余的小时
          var minutes = parseInt(leftTime / 1000 / 60 % 60, 10); //计算剩余的分钟
          var seconds = parseInt(leftTime / 1000 % 60, 10); //计算剩余的秒数
          if (seconds < 0) {
            return text
          }
          days = this.checkTime(days)
          hours = this.checkTime(hours)
          minutes = this.checkTime(minutes)
          seconds = this.checkTime(seconds)
          if (days) {
            text = days + "天" + hours + "时" + minutes + "分"
          } else {
            text = hours + "时" + minutes + "分" + seconds + "秒"
          }
          return text
        },
        checkTime(i) { //将0-9的数字前面加上0，例1变为01
          if (i < 10) {
            i = "0" + i
          }
          return i
        }
      }
    })
  </script>
</body>

</html>