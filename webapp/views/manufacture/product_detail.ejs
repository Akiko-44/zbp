<style>
.column1{background: #F5F5F5;height:500px;}
.column1 .imgs{height:500px;width: 500px;position: relative;z-index: 100;}
.column1 .imgs+div{margin-left: 500px;height:100%;position:relative;}
.column1 .imgs+div>div{position:absolute;top: 50%;left:50%;transform: translate(-50%,-50%);}
.column1 p {line-height: 28px;margin:38px 0;}

.column2{padding:20px;border-bottom:2px solid #D6D6D6;margin-bottom:20px;}
.column2 .el-input__inner {height:29px;line-height: 29px;}

.columnVideo{padding: 20px;}

.fs30{font-size: 30px;}
.scanInfo {width: 250px;}
.scanList li{margin: 10px;cursor: pointer;}
.scanList li p {line-height: 20px;}

.detailNav{padding: 18px 0;text-align: center;}
.detailNav li{display:inline-block;padding:0 60px;font-size:16px;color:#666;border-left: 1px solid #D6D6D6;}
.detailNav li:first-child{border-left: 0;}
.detailNav li.cur{color:#E54756;}

.detail3 td {border: 1px solid #efefef;padding: 12px;color:#666;}
.collect{
	padding: 5px 10px;
	background: #999999;
	color: #FFFFFF;
	font-size: 14px;
	cursor: pointer;
	position: absolute;
	z-index: 9999999;
	top: 50px;
	right: 10px;
	border-radius: 5px;
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
}
.collected{
	background: #ff9900;
}
/*商品规格*/
.specification{
	line-height: 27px;
}
.specification .norms{
	    display: inline-block;
}
.specification .norms .norm{
	    border: 1px solid #CCCCCC;
    padding: 3px 12px;
    margin-left: 5px;
        position: relative;
    cursor: pointer;
}
.specification .norms .norm i{
	display: inline-block;
    	display: none;
    	width: 8px;
    	height: 8px;
    	background: url(../../src/img/jewelry/default_adress.png) no-repeat;
    	background-size: 100% auto;
    	position: absolute;
    	right: 0;
    	bottom: 0;
    }
.specification .norms .norm:hover{
	border-color: #E6B75B;
}
.specification .norms .norm.active{
	    border: 1px solid #E6B75B;
    padding: 3px 12px;
    margin-left: 5px;
}
.specification .norms .norm.active i{
	    display: block;
}
.specification,.goodsNum,.goodsStock{
	margin-right: 15px;
	    margin-top: 6px;
}
.goodsStock .el-input{
	width: 100px;
}
#coupons_select{
	display: inline-block;
	width: 100px;
	height: 40px;
	line-height: 40px;
	text-align: center;
	border: 1px solid #C2A347;
	border-radius: 4px;
	-webkit-border-radius: 4px;
	-moz-border-radius: 4px;
	background: #f5dab1;
	color: #e6a23c;
	-webkit-transform: translateY(4px);
	-moz-transform: translateY(4px);
	-ms-transform: translateY(4px);
	-o-transform: translateY(4px);
	transform: translateY(4px);
	margin-right: 10px;
	cursor: pointer;
}
#coupons{
	border: 1px dashed #E5E5E5;
	padding: 20px;
	margin-bottom: 20px;
}
#coupons button{
	border: none;
	padding: 5px;
	margin-right: 10px;
	color: #FFFFFF;
	background: #E6B75B;
	border-radius:3px;
	-webkit-border-radius:3px;
	-moz-border-radius:3px;
}
#coupons button.poor{
	background: #CCCCCC;
}
#coupons button:hover{
	opacity: .7;
}
/*关联商品*/
#correlation{border-bottom: 1px solid #E5E5E5;margin-bottom: 20px;}
#correlation .correlation{
	font-size: 16px;
	padding: 20px;
	background: #F5F5F5;
}
#correlation .colligate p{
	margin-bottom: 10px;
}
#correlation .colligate_slider {
	width: 720px;
	overflow: hidden;
}
#correlation .colligate_item{
	padding: 0 45px;
}
#correlation .colligate_goodsName{
	font-size: 14px;
	line-height: 16px;
	height: 32px;
	display: -webkit-box;
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 2;
	overflow: hidden;
}
#correlation .colligate_price{
	font-size: 16px;
	color: #890E1C;
}
#correlation .colligate_desc{
	margin: 10px 0;
}
#correlation .colligate_desc span{
	color: #C2A374;
	border: 1px solid #C2A374;
	padding: 1px 5px;
	border-radius: 3px;
	margin-right: 5px;
} 
.bdsharebuttonbox{position: absolute;top: 0px;right: 0;}

.statusWrap{
	display: inline-block;
	height: 25px;
	line-height: 25px;
	background: #FF0000;
	padding: 0 5px;
	text-align: center;
	font-size: 12px;
	color: #FFFFFF;
	vertical-align: middle;
	border-radius: 5px;
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
}

/*jqzoom*/
.slick-list.draggable{
	overflow: initial;
}
.zoomdiv{
	z-index:100;
	position:absolute;
	top:0px;left:530px;
	width:500px;
	height:500px;
	 border:1px solid #e4e4e4;
	 display:block !important;
	 text-align:center;
	 overflow: hidden;
	 background: #FFF;
	 }
