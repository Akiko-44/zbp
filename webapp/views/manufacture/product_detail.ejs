function buy2(){
	var arr = [{
		goodsId: detailVm.skuObj.goodsId,
		skuId: detailVm.skuObj.id,
		goodsName: detailVm.data.caseName,
		mainPic: detailVm.mainPic,
		price: detailVm.skuObj.price,
		goodsNum: detailVm.num
	}], totalPrice = detailVm.skuObj.price;
	var associateGoodsId = detailVm.data.associateGoodsId;
	$.each($(".colligate_check"), function(idx, item) {
		if($(item).is(":checked")){
			var index = Number($(item).val())
			var associate = associateGoodsId[index]
			arr.push({
				goodsId: associate.goodsid,
				skuId: associate.skuid,
				goodsName: associate.goodsName,
				mainPic: associate.mainPic,
				price: associate.price,
				goodsNum: 1
			})
			totalPrice += Number(associate.price)
		}
	});
	if(arr.length){
		var freightPrice = $(".colligate_freightPrice").val();
		var obj = JSON.stringify({'freightPrice':freightPrice,'totalPrice':totalPrice,'merchantName': $(".shop-title").text(),'arr':arr});
		window.location = '../../exchange/buy?flag=skuGoods&source=2&obj='+obj
	}else{
		utils.alert('danger','请选择您要购买的商品！')
	}
}
